<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ResultatActivity"
    android:orientation="vertical"
    android:background="@drawable/tavle2">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="50dp"
        android:text="@string/ingen_flere_tilgjengelige_sp_rsm_l"
        android:textSize="18sp"
        android:fontFamily="@font/coming_soon"
        />

    <ImageView
        android:id="@+id/smilefjes"
        android:layout_width="215dp"
        android:layout_height="97dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="40dp"
        app:srcCompat="@drawable/meh" />

    <TextView
        android:id="@+id/tilbakemelding"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="25dp"
        android:text="Tilbakemelding"
        android:textSize="18sp"
        android:fontFamily="@font/coming_soon"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView4"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="100dp"
            app:srcCompat="@drawable/thumbsup" />

        <TextView
            android:id="@+id/antRiktSvarResTextView"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="0"
            android:textAlignment="center"
            android:textSize="18sp"
            android:fontFamily="@font/coming_soon" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">

        <ImageView
            android:id="@+id/imageView3"
            android:layout_width="50dp"
            android:layout_height="30dp"
            android:layout_marginLeft="100dp"
            app:srcCompat="@drawable/thumbsdown" />

        <TextView
            android:id="@+id/antFeilSvarResTextView"
            android:layout_width="50dp"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="0"
            android:textAlignment="center"
            android:textSize="18sp"
            android:fontFamily="@font/coming_soon" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="50dp"
        android:orientation="horizontal"
        >

        <Button
            android:id="@+id/hovedmenyResButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginLeft="20dp"
            android:layout_weight="1"
            android:backgroundTint="#0FFFFFFF"
            android:fontFamily="@font/coming_soon"
            android:text="@string/hovedmeny"
            android:textStyle="bold" />

        <Button
            android:id="@+id/spillPaaNyttResButton"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginRight="20dp"
            android:layout_weight="1"
            android:backgroundTint="#0FFFFFFF"
            android:fontFamily="@font/coming_soon"
            android:text="@string/spill_p_nytt"
            android:textStyle="bold" />
    </LinearLayout>

</LinearLayout>